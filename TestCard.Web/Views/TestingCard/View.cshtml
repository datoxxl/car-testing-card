@model TestCard.Web.Models.TestingCardModel

@{
    ViewBag.Title = "View";
}
<div class="x-form-container">
    <span class="header">@GeneralResource.TestCard</span>
    <div class="top-actions">
        @Html.ActionLink(GeneralResource.BackToList, "List")
        @Html.ActionLink(GeneralResource.TestCard, "View", ViewContext.RouteData.Values, new { @class = "active" })
        @Html.ActionLink(GeneralResource.Images, "Images", ViewContext.RouteData.Values)
        @{ Html.RenderPartial("_FormActions", new List<ActionButton> {
            new ActionButton
            {
                Controller = "Print",
                Action = "TestingCard",
                ObjectID = Model.TestingCardID,
                Type = ActionButton.ActionTypes.Print
            }
            }); }
    </div>
    <div class="form-inner">
        @Html.DisplayForModel()
    </div>
</div>
