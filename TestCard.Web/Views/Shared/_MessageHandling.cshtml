@if (TempData["ErrorMessage"] != null)
{
    <script>
        $(function () {
            showError('@TempData["ErrorMessage"]', '');
        });
    </script>
}
else if (TempData["SuccessMessage"] != null)
{
    <script>
        $(function () {
            showSuccess('@TempData["SuccessMessage"]', '');
        });
    </script>
}
