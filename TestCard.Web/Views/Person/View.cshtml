@model TestCard.Web.Models.PersonModel

@{
    ViewBag.Title = "View";
}
<div class="x-form-container">
    <span class="header">@GeneralResource.User</span>
    <div class="top-actions">
        @Html.ActionLink(GeneralResource.BackToList, "List")
        @Html.ActionLink(GeneralResource.User, "View", "Person", ViewContext.RouteData.Values, new { @class = "active" })
        @Html.ActionLink(GeneralResource.WorkSchedule, "View", "PersonSchedule", ViewContext.RouteData.Values, null)
    </div>
    <div class="form-inner">
        @Html.DisplayForModel()
    </div>
</div>
